{{ range .items -}}
{{- $metadata := .metadata -}}
{{- $clusterIP := .spec.clusterIP -}}
{{- range .spec.ports }}        {
            "name": "out.{{ $metadata.namespace }}.{{ $metadata.name }}.{{ .port }}",
            "type": "static",
            "lb_type": "round_robin",
            "connect_timeout_ms": 1000,
            "hosts": [
                {
                    "url": "tcp://{{ $clusterIP }}:{{ .port }}"
                }
            ]
        },
{{ end -}}
{{ end }}
